@model Department
@{
	ViewData["Title"] = "Delete";
}

<h1>Are Yor Sure You Want To Delete @Model.Name Department </h1>
<br />
<div asp-validation-summary="All" class="text-bg-danger"></div>
<form asp-action="Delete">
	
	<partial name="PartialViews/_DeleteDetailsPartialView" />
	<input type="submit" value="Delete" class="btn btn-danger" /> |
	<a class="btn btn-light" asp-action="Index" asp-controller="Department">Cancel</a>
</form>

